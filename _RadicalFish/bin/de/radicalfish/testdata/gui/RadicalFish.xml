        <area name="checkbox.bg2" xywh="48,14,16,16" splitx="L4,R4" splity="T4,B4"/>
        <animation name="checkbox.show" timeSource="selected" count="1">
            <frame duration="50" ref="checkbox.on" zoom="0.20050125"/>
            <frame duration="50" ref="checkbox.on" zoom="0.40025064"/>
            <frame duration="50" ref="checkbox.on" zoom="0.6007519"/>
            <frame duration="50" ref="checkbox.on" zoom="0.80125314"/>
            <frame duration="10000" ref="checkbox.on"/>
        </animation>
        <select name="checkbox.background">
            <composed if="selected + armed">
                <alias ref="checkbox.bg2"/>
                <alias ref="checkbox.on"/>
            </composed>
            <composed if="selected">
                <alias ref="checkbox.bg1"/>
                <alias ref="checkbox.show" center="true"/>
            </composed>
            <alias ref="checkbox.bg2" if="armed"/>
            <alias ref="checkbox.bg1"/>
        </select>
    
        <grid name="checkbox" weightsX="0,1" weightsY="1">
            <select>
                <composed if="selected + armed">
                    <alias ref="checkbox.bg2"/>
                    <alias ref="checkbox.on"/>
                </composed>
                <composed if="selected">
                    <alias ref="checkbox.bg1"/>
                    <alias ref="checkbox.show"/>
                </composed>
                <alias ref="checkbox.bg2" if="armed"/>
                <alias ref="checkbox.bg1"/>
            </select>
            <alias ref="none"/>
        </grid>
    
