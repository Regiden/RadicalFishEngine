        <area name="radiobox.bg1" xywh="14,31,16,16"/>
        <area name="radiobox.bg2" xywh="31,31,16,16"/>
    
    
        <animation name="radiobox.show" timeSource="selected" count="1">
            <frame duration="50" ref="radiobox.on" zoom="0.20050125"/>
            <frame duration="50" ref="radiobox.on" zoom="0.40025064"/>
            <frame duration="50" ref="radiobox.on" zoom="0.6007519"/>
            <frame duration="50" ref="radiobox.on" zoom="0.80125314"/>
            <frame duration="10000" ref="radiobox.on"/>
        </animation>
