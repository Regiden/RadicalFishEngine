                </composed>
                <composed if="selected">
                    <alias ref="radiobox.bg1"/>
                    <alias ref="radiobox.show"/>
                </composed>
                <alias ref="radiobox.bg2" if="armed"/>
                <alias ref="radiobox.bg1"/>
            </select>
            <alias ref="none"/>
        </grid>
